{
  "name": "fastify-angular",
  "version": "0.3.3",
  "description": "Fastify plugin for angular universal project w/ multiple locales",
  "keywords": [
    "fastify",
    "angular",
    "angular-universal",
    "server-side-rendering",
    "SSR"
  ],
  "main": "index.js",
  "repository": "https://github.com/clarkorz/fastify-angular",
  "author": "Clark Wang <clark.wangs@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "tap test/*.test.js test/*/*.test.js test/*/*/*.test.js",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -l info -P app.js"
  },
  "dependencies": {
    "@angular/common": ">=6.0.0",
    "@nguniversal/common": ">=6.0.0",
    "@nguniversal/module-map-ngfactory-loader": ">=6.0.0",
    "fastify-plugin": ">=1.2.0",
    "fastify-static": ">=0.14.0"
  },
  "devDependencies": {
    "fastify": "^2.1.0",
    "fastify-autoload": "^0.6.0",
    "fastify-cli": "^1.0.0",
    "tap": "^12.6.0"
  },
  "peerDependencies": {
    "@angular/animations": ">=6.0.0",
    "@angular/compiler": ">=6.0.0",
    "@angular/core": ">=6.0.0",
    "@angular/forms": ">=6.0.0",
    "@angular/http": ">=6.0.0",
    "@angular/platform-browser": ">=6.0.0",
    "@angular/platform-browser-dynamic": ">=6.0.0",
    "@angular/router": ">=6.0.0",
    "fastify": ">=2.1.0"
  }
}
