{
  "name": "fastify-angular-root",
  "private": true,
  "version": "0.1.0",
  "description": "Fastify plugin for angular universal project w/ multiple locales",
  "keywords": [
    "fastify",
    "angular",
    "angular-universal",
    "server-side-rendering",
    "SSR"
  ],
  "repository": "https://github.com/clarkorz/fastify-angular",
  "author": "Clark Wang <clark.wangs@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "tap test/*.test.js test/*/*.test.js test/*/*/*.test.js",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -l info -P app.js"
  },
  "devDependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/platform-server": "^7.0.0",
    "@angular/router": "^7.0.0",
    "fastify": "^1.12.0",
    "lerna": "^3.7.2",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "workspaces": [
    "packages/*"
  ]
}
